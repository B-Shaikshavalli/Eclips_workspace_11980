package collections;

import java.util.ArrayList;

public class ArrayListDemo {
	public static void main(String[] args) {
		ArrayList a1 = new ArrayList();
		System.out.println(a1.size());
		
		a1.add(100);
		a1.add(200);
		System.out.println(a1.size());
		System.out.println(a1);
		
		a1.add("java");
		a1.add("Python");
		System.out.println(a1.size());
		System.out.println(a1);
	}

}
